{
  "name": "jibaru/nubefact-api",
  "description": "Nubefact API Integration",
  "type": "library",
  "require": {
    "guzzlehttp/guzzle": "^7.5",
    "fakerphp/faker": "^1.21"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "csniff": "vendor/bin/phpcs --standard=PSR12 src/ tests/",
    "csbf": "vendor/bin/phpcbf --standard=PSR12 src/ tests/",
    "test": "vendor/bin/phpunit tests"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Jibaru\\NubefactApi\\": "src/",
      "Jibaru\\NubefactApi\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jibaru",
      "email": "ignacioruedaboada@hotmail.com"
    }
  ]
}
